@model IEnumerable<Atlas.Models.ViewModels.MushroomsListVM>

<h1>Nasze Grzybki</h1>

<ul>
    @foreach (var item in Model)
    {
        <li>
            <a asp-area="" asp-action="Detail" asp-route-url="@item.Url">
                <img src="~/img/@item.Image" alt="tutaj powinien być obrazek">
            @item.Name</a>
        </li>
    }
</ul>